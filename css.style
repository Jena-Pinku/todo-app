body {
    font-family: Arial, sans-serif;
    text-align: center;
    background-color: #f4f4f4;
}

.container {
    width: 300px;
    margin: 100px auto;
    background: white;
    padding: 20px;
    box-shadow: 0px 0px 10px gray;
}

button {
    margin-left: 10px;
}

ul {
    list-style: none;
    padding: 0;
}

li {
    background: #eee;
    padding: 5px;
    margin: 5px;
    display: flex;
    justify-content: space-between;
}
